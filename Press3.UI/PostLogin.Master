    <link href="assets/admin/layout/css/custom.css" rel="stylesheet" type="text/css" />
    <!-- END THEME STYLES -->
    <style type="text/css">
        .notifctn{
            width: 15px;
            height: 15px;
            background: red;
            position:absolute;
            left: 33px;
            top: 70px;
            border-radius: 100% !important;
            padding: 1px;  
            text-align: center;
            color:#fff;
            line-height: 13px;
            font-size: 9px;
        }
        .cbr-notfctn{
            width: 17px;
            height: 17px;
            background: red;
            float: right;
            border-radius: 100% !important;
            color: #fff;
            font-size: 9px;
            text-align: center;
            margin-top: 2px;
            line-height: 17px;
        }
        @media (min-width:992px) {
            .navbar-fixed-top {
                width: 97%;
                left: 65px;
            }
        }

        .pad-left-0 {
            padding-left: 0px;
        }

        .pad-left-5 {
            padding-left: 5px;
        }

        .dropdown-menu li.status_agent span {
            padding: 8px 14px;
            line-height: 29px;
            cursor: pointer;
        }

        div.checker {
            display: inline;
        }

        .page-footer-inner a {
            color: #337ab7;
            text-decoration: none;
        }
         .blockUI  > img{
            height:80px;
            width:80px;
            display:block;
            margin-left:auto;
            margin-right:auto;
            align-content:center;
          
        }
         .blockUI{

             width:100% !important;
             left:auto !important;
               border:none;
            background-color:transparent;
         }

    </style>
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
</head>
<body class="page-quick-sidebar-over-content page-style-square page-sidebar-closed">
    <input id="hdnAgentName" type="hidden" value="<%= agentName %>" />
    <input id="hdnRoleId" type="hidden" value="<%= roleId %>" />
    <!-- BEGIN CONTAINER -->
    <div class="page-container">
        <!-- BEGIN SIDEBAR -->
        <div class="page-sidebar-wrapper">
            <div class="page-sidebar navbar-collapse collapse">

                <ul class="page-sidebar-menu page-sidebar-menu-closed" data-keep-expanded="false" data-auto-scroll="true" data-slide-speed="200">
                    <!-- DOC: To remove the sidebar toggler from the sidebar you just need to completely remove the below "sidebar-toggler-wrapper" LI element -->
                    <li class="sidebar-toggler-wrapper">
                        <!-- BEGIN SIDEBAR TOGGLER BUTTON -->
                        <!--<div class="sidebar-toggler">
					</div>-->
                        <!-- END SIDEBAR TOGGLER BUTTON -->
                    </li>
                    <!-- DOC: To remove the search box from the sidebar you just need to completely remove the below "sidebar-search-wrapper" LI element -->

                    <li class="" id="SideBarToggle1">
                        <% if (roleId == 1)
                           { %>
                        <a href="/AgentHome.aspx" class="nav-link " />
                        <% }
                           else if (roleId == 3)
                           { %>
                        <a href="/ManagerDashboard.aspx" class="nav-link " />
                        <% }
                           else if (roleId == 2)
                           { %>
                        <a href="/SupervisorDashboard.aspx" class="nav-link " />
                        <% }
                           else if (roleId == 4)
                           { %>
                        <a href="/TicketManagement.aspx" class="nav-link " />
                        <% }
                           else
                           { %>
                        <a href="javascript:;">
