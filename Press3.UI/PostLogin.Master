<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="PostLogin.master.cs" Inherits="Press3.UI.PostLogin" %>

<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <meta charset="utf-8" />
